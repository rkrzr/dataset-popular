#newsbulletin{
	width:310px;
	margin-bottom:10px;
} 

.indexv2 #newsbulletin {
    margin-bottom:20px;
}

#newsbulletin .midcontainer{
	background:#fff;
}

#newsbulletin a {
	text-decoration: none;
	color: #369;
}

#newsbulletin a:hover { text-decoration: underline; }

#newsbulletin .widget_head{
    height:auto;
	width:310px;
	border-top:none;
	border-bottom: 1px solid #bcbcbc;
	text-align:center;
	padding:0 0 1px;
	margin:0;
}

#newsbulletin .widget_head .title { font-size: 24px; text-transform: uppercase; font-weight:normal; font-family: georgia, 'times new roman', sans-serif;}

#newsbulletin .widget_head .twitterLink { font-size:14px; font-family: georgia, 'times new roman', sans-serif; }
#newsbulletin .twitterLink .twitterContent { display:inline-block; margin-bottom: 3px; }
#newsbulletin .twitterContent .linkHeadline, #newsbulletin .twitterContent .linkImg { display:block; float:left; }
#newsbulletin .twitterContent .linkImg { padding-left: 5px; }

#newsbulletin .nbheader{
	padding-top:3px;
	font:normal 14px/18px georgia, 'times new roman', sans-serif;
	color:#000;
	background:#fff;
	width:295px;
}
#newsbulletin .nbheader a {font:normal 14px/18px georgia, 'times new roman', serif;}
#newsbulletin .nbheader a:hover {text-decoration:underline;}

#newsbulletin #nbScrollPane{
	background:#fff;
	overflow: hidden;
	height:350px;
	width:310px; /* +3 to accomodate scrollbar offset */
	padding-top:5px;
	outline:none;
}

#newsbulletin #nbItemContainer{padding-top:2px;
	height:auto;
}

#newsbulletin .nbItem{
	padding:2px 3px 2px 0px;
	background:#fff;
	width:295px;
	font-size: 14px;
    line-height: 1.3em;
    font-family: georgia, 'times new roman', serif;
}

#newsbulletin .nbItemContentsAuthor{
	height:auto !important;
	height:90px;
	min-height:90px;
}

#newsbulletin .nbTimeStamp{
	font:normal 11px arial, sans-serif;
	color:#666;
	padding:2px 0 0;
}

