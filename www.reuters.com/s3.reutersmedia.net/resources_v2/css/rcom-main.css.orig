/**********/
/* Topics */
/**********/

.topicLinks ul {
	float:left;
	clear:both;
	margin:0 0 20px;
	padding:0 0 10px;
	text-align:center;
}

.topicLinks li,
.actionButton {
	background-position: 0 -95px;
