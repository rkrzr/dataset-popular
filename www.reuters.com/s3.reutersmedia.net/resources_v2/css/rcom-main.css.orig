}

.relatedInfo .timestamp {
	font-size: 10px;
	color: #999;
}

/*****************/
/* Breaking News */
/*****************/
#breakingNewsBand {	position: relative; top: -10px; margin: 0;}
#breakingNewsBand .sectionContent {position: relative; margin: 0 auto; padding: 0; width: 100%;}
#breakingNewsBand .sectionColumns {position: relative; margin:0; display:block;}
#breakingNewsBand .grid12 {position: relative; width: 100%;}
#breakingNewsBand .gridPanel {position: relative; margin: 0; float:none;}

#breakingNewsContainer {
	background: #c10909 url("/resources_v2/images/bg_breaking_bg.gif") top left repeat-x;
	position: relative;
	margin: 0;
	padding: 0;
	top: -1px;
	height: 1%;
	display: block;
}
html>body #breakingNewsContainer {
	height: auto;
}

#breakingNewsUltra {position: relative; display: block; padding: 14px 0;}
#breakingNewsContainer #breakingNewsUltra h1,
#breakingNewsContainer #breakingNewsUltra h1 a {
	font-size: 16px;
	color: #FFF;
	font-weight: bold;
	margin: 0;
	padding: 0;
	display: inline;
	line-height: normal;
}

.developingStory h1,
.developingStory h1 a,
.developingStory h1 a:visited,
.developingStory h1 .breakingNewsLabel {
	color: #333 !important;
}

.happening h1,
.happening h1 a,
.happening h1 a:visited,
.happening h1 .breakingNewsLabel {
	color: #FFF !important;
}

.breakingStory h1,
.breakingStory h1 a,
.breakingStory h1 a:visited,
.breakingStory h1 .breakingNewsLabel {
	color: #FFF !important;
}

#breakingNewsContainer #breakingNewsUltra h1 .breakingNewsLabel {
	display: inline;
	padding: 0 0 2px 0;
	margin: 0;
	font-size: 16px;
	font-weight: bold;
	color: #FFF;
	text-transform: uppercase;
}
#breakingNewsContainer #breakingNewsUltra .breakingNewsContent {
	width: 940px;
	margin: 0 auto;
}

/* edits for gray 'Developing Story' and 'Happening Now' band */

.developingStory {
	background: #b5b5b5 url("/resources_v2/images/bg_developing_bg.gif") top left repeat-x !important;
}

.happening {
	background: #006185 url("/resources_v2/images/bg_live_bg.gif") top left repeat-x !important;
}

.happening #breakingNewsUltra, .developingStory #breakingNewsUltra {padding: 14px 0;}

.happening #breakingNewsUltra .breakingNewsContent, .developingStory #breakingNewsUltra .breakingNewsContent {
	width: 940px;
	margin: 0 auto;
}


